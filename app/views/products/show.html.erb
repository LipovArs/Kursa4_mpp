<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>DedDoma</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  <link rel="stylesheet" href="home.scss">
  <link rel="icon" href="favicon.png" sizes="any"/>
</head>

<body>
<p>
  <strong>Pr name:</strong>
  <%= @product.pr_name %>
</p>

<p>
  <strong>Price for client:</strong>
  <%= @product.price_for_client %>
</p>

<p>
  <strong>Description:</strong>
  <%= @product.description %>
</p>

<p>
  <strong>Skun:</strong>
  <%= @product.SKUN %>
</p>

<p>
  <strong>Volume:</strong>
  <%= @product.pr_volume_id %>
</p>

<p>
  <strong>Alcohol:</strong>
  <%= @product.pr_alcohol_id %>
</p>

<p>
  <strong>Endurance:</strong>
  <%= @product.pr_endurance_id %>
</p>

<p>
  <strong>Pr category:</strong>
  <%= @product.pr_category_id %>
</p>

<p>
  <strong>Pr sub category:</strong>
  <%= @product.pr_sub_category %>
</p>

<p>
  <strong>Pr brand:</strong>
  <%= @product.pr_brand_id %>
</p>

<p>
  <strong>Pr country:</strong>
  <%= @product.pr_country_id %>
</p>

<p>
  <strong>Pr color:</strong>
  <%= @product.pr_color_id %>
</p>

<p>
  <strong>Pr sweetness:</strong>
  <%= @product.pr_sweetness_id %>
</p>
</body>
</html>
