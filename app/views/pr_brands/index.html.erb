<p id="notice"><%= notice %></p>

<h1>Pr Brands</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pr_brands.each do |pr_brand| %>
      <tr>
        <td><%= pr_brand.brand %></td>
        <td><%= link_to 'Show', pr_brand %></td>
        <td><%= link_to 'Edit', edit_pr_brand_path(pr_brand) %></td>
        <td><%= link_to 'Destroy', pr_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr Brand', new_pr_brand_path %>
