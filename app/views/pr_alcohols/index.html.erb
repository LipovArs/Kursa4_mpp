<p id="notice"><%= notice %></p>

<h1>Pr Alcohols</h1>

<table>
  <thead>
    <tr>
      <th>Alcohol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pr_alcohols.each do |pr_alcohol| %>
      <tr>
        <td><%= pr_alcohol.alcohol %></td>
        <td><%= link_to 'Show', pr_alcohol %></td>
        <td><%= link_to 'Edit', edit_pr_alcohol_path(pr_alcohol) %></td>
        <td><%= link_to 'Destroy', pr_alcohol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr Alcohol', new_pr_alcohol_path %>
