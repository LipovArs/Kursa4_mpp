<p id="notice"><%= notice %></p>

<h1>Pr Classifications</h1>

<table>
  <thead>
    <tr>
      <th>Classification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pr_classifications.each do |pr_classification| %>
      <tr>
        <td><%= pr_classification.classification %></td>
        <td><%= link_to 'Show', pr_classification %></td>
        <td><%= link_to 'Edit', edit_pr_classification_path(pr_classification) %></td>
        <td><%= link_to 'Destroy', pr_classification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr Classification', new_pr_classification_path %>
