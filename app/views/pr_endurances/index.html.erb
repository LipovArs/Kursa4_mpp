<p id="notice"><%= notice %></p>

<h1>Pr Endurances</h1>

<table>
  <thead>
    <tr>
      <th>Endurance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pr_endurances.each do |pr_endurance| %>
      <tr>
        <td><%= pr_endurance.endurance %></td>
        <td><%= link_to 'Show', pr_endurance %></td>
        <td><%= link_to 'Edit', edit_pr_endurance_path(pr_endurance) %></td>
        <td><%= link_to 'Destroy', pr_endurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr Endurance', new_pr_endurance_path %>
